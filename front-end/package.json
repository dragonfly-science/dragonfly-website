  "scripts": {
    "analyze": "npm run eslint:check && npm run ts:lint && npx unimported",
    "postanalyze": "npm run eslint:fix && npm run prettier:fix",
    "prebuild": "rm -f ../_site/assets/* && npm run fonts",
    "build": "NODE_ENV=production webpack --progress --env prod --config ./webpack.prod.js",
    "build:static": "webpack --progress --env dev --config ./webpack.dev.js",
    "check-types": "tsc",
    "eslint": "eslint \"./src/js/**/*.ts\"",
    "eslint:check": "eslint --print-config \"./src/js/**/*.ts\" | eslint-config-prettier-check",
    "eslint:fix": "eslint --fix \"./src/js/**/*.ts\"",
    "fonts": "mkdir -p ../content/fonts & glyphs2font fonts.yaml",
    "postfonts": "mkdir -p ../_site/fonts && postcss ../content/fonts/dragonfly.css --env production --no-map -o ../_site/fonts/dragonfly.css",
    "imagemin": "chmod -R 775 ../_site && node imagemin.js",
    "prettier:fix": "prettier --write \"./src/js/**/*.ts\"",
    "prettier:lint": "prettier --check \"./src/js/**/*.ts\"",
    "staging": "serve -l 3000",
    "ts:lint": "tslint -c tslint.json 'src/js/**/*.ts'",
    "prewatch": "rm -f ../_site/assets/* && npm run fonts",
    "watch": "webpack --env dev --watch --config ./webpack.dev.js"
  },
  "author": "Dragonfly Data Science",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
